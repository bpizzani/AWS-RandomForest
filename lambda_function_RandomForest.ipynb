def resolution(res):
    if (res == "Buyer Fraud  Loss") or (res == "Chargeback Received - Fraud") or (res == "ATO-Buyer Loss") or (res == "ATO-Buyer Prevented") or (res == "Chargeback - Hostile Fraud Missed"):
        return "Fraud"
    elif (res == "Buyer Fraud Prevented") :
        return "Suspicious"
    else:
        return "No Fraud"
    
